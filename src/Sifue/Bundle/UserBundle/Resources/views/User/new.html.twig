{% extends 'SifueMenuBundle:Menu:menu.html.twig' %}

{% block title %}ユーザー作成{% endblock %}

{% block menu_content %}

<h1>ユーザー作成</h1>

<form action="{{ path('user_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">作成</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('user') }}">
            一覧へ戻る
        </a>
    </li>
</ul>

{% endblock %}
